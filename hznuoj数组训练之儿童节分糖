import java.util.Scanner;
public class Main {
	public static void main(String args[]){
		Scanner sc= new Scanner(System.in);
		int t=sc.nextInt();
		int[] a=new int[10005];
		for(int i=0;i<t;i++) {
			int n=sc.nextInt();
			int k=sc.nextInt();
			for(int j=0;j<n;j++) {
				a[j] = sc.nextInt();
				if(a[j] == k) {
					System.out.println(j+1);
				}
			}
			
		}
	}
}
